  },
  "keywords": [
    "language",
    "learning",
    "read"
  ],
  "author": "Chiazo",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "lite-server": "^2.5.3",
    "typescript": "^3.5.1"
  }
}
